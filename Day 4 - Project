SOLIDITY CODE
pragma solidity >= 0.4.12 < 0.6.12;

contract ReportCard{
    
    string public StudentName;
    string public RollNo;
    string public Year;
    uint public TotalMarks;
    string public Status;
    
    function ReportCard(string newStudentName, string newRollNo, string newYear, uint newTotalMarks, string newStatus) public{
        StudentName = newStudentName;
        RollNo = newRollNo;
        Year = newYear;
        TotalMarks = newTotalMarks;
        Status = newStatus;
    }
    function setDetails(string newStudentName, string newRollNo, string newYear, uint newTotalMarks, string newStatus) public{
        StudentName = newStudentName;
        RollNo = newRollNo;
        Year = newYear;
        TotalMarks = newTotalMarks;
        Status = newStatus;
    }
    function getDetails() public view returns(string,string,string,uint,string){
        return (StudentName,RollNo,Year,TotalMarks,Status);
    }
 
}


